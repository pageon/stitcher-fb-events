{
    "name": "pageon/stitcher-fb-events",
    "description": "Sync Facebook events",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "BrenDt",
            "email": "brent@pageon.be"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0",
        "pageon/stitcher-core": "@beta",
        "guzzlehttp/psr7": "^1.4",
        "nesbot/carbon": "^1.22",
        "guzzlehttp/guzzle": "^6.2"
    },
    "autoload": {
        "psr-4" : {
            "Pageon\\Stitcher\\" : "src/Pageon/Stitcher"
        }
    },
    "autoload-dev": {
